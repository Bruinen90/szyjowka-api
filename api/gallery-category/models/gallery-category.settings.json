{
  "kind": "collectionType",
  "collectionName": "gallery_categories",
  "info": {
    "name": "Gallery category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3,
      "maxLength": 60
    },
    "target": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
